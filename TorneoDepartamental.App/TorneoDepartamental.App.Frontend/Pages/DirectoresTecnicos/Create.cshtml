@page
@using TorneoDepartamental.App.Dominio
@model TorneoDepartamental.App.Frontend.Pages.DirectoresTecnicos.CreateModel
@{
}
<h1>Nuevo DT</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="tecnico.Nombre"></label>
        <input asp-for="tecnico.Nombre" class="form-control">
        <span asp-validation-for="tecnico.Nombre" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="tecnico.Telefono"></label>
        <input asp-for="tecnico.Telefono" class="form-control">
        <span asp-validation-for="tecnico.Telefono" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="tecnico.Documento"></label>
        <input asp-for="tecnico.Documento" class="form-control">
        <span asp-validation-for="tecnico.Documento" class="text-danger"></span>
    </div>
     <button type="submit" class="btn btn-primary">Crear</button>
</form> 
<a class="btn btn-secondary" asp-page="./Index">Volver</a>
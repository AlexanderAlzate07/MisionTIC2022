@page
@model TorneoDepartamental.App.Frontend.Pages.Equipos.EditModel
@{
}
<h1>Editar Equipo</h1>
<form method="post">
    <input type="hidden" asp-for="equipo.Id">
    <div class="form-group">
        <label asp-for="equipo.Nombre"></label>
        <input asp-for="equipo.Nombre" class="form-control">
        <span class="text-danger" asp-validation-for="equipo.Nombre"></span>
    </div>

    <button type="submit" class="btn btn-primary">Actualizar Equipo</button>
</form>
<a class="btn btn-primary" asp-page="./Index">Volver</a>
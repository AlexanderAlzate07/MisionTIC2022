@page
@model TorneoDepartamental.App.Frontend.Pages.Equipos.CreateModel
@{
}
<h1>Nuevo Equipo</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="equipo.Nombre"></label>
        <input asp-for="equipo.Nombre" class="form-control">
        <span class="text-danger" asp-validation-for="equipo.Nombre"></span>
    </div>

    <button type="submit" class="btn btn-primary">Crear Equipo</button>
</form>
<a class="btn btn-primary" asp-page="./Index">Volver</a>
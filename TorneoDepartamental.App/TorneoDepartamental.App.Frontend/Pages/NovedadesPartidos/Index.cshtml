@page
@model TorneoDepartamental.App.Frontend.Pages.NovedadesPartidos.IndexModel
@{
}
<h1>Listado de Novedades de Partidos</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            @* <th scope="col">Fecha Partido</th> *@
        </tr>
    </thead>
    <tbody>
        @foreach (var novPartido in Model.NovedadesPartidos)
        {
            <tr>
                <td>@novPartido.Id</td>
                @* <td>@novPartido.Partido.FechaPartido</td> *@
                <td><a class="btn btn-primary" asp-page="./Details" asp-route-id="@novPartido.Id">Detalle</a></td>
            </tr>
        }
    </tbody>
</table>